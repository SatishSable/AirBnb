# Mapbox Token (for maps functionality)
Map_Token=your_mapbox_token_here

# Cloudinary Configuration (for image uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_KEY=your_cloudinary_api_key_here
CLOUDINARY_SECRET=your_cloudinary_api_secret_here

# MongoDB Atlas URL (for production) or local MongoDB
ATLASDB_URL=mongodb://127.0.0.1:27017/Wanderlust

# Session Secret (use a strong random string in production)
SESSION_SECRET=your_session_secret_here

# Google OAuth (optional - for social login)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALLBACK_URL=http://localhost:8080/auth/google/callback

# Server Port (optional, defaults to 8080)
PORT=8080
